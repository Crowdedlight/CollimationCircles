<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:CollimationCircles.ViewModels"
             xmlns:controls="clr-namespace:CollimationCircles.Controls"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="450"
             x:Class="CollimationCircles.Controls.LicenseUserControl"
             x:DataType="vm:AboutViewModel">
  
    <WrapPanel      
      Margin="8 0 8 0">
      <TextBlock
        Margin="0 0 8 0"
        Text="{DynamicResource Text.License}"/>
      <TextBlock
        TextWrapping="WrapWithOverflow"
        Text="{ReflectionBinding License}"/>
    </WrapPanel>

</UserControl>
